    <sql>CREATE TABLE IF NOT EXISTS `directories` (
  id smallint(5) unsigned NOT NULL auto_increment PRIMARY KEY,
  `common_path` varchar(500) NOT NULL,
  `im-log03_path` varchar(500) default NULL,
  `im-log02_path` varchar(500) default NULL,
  `scan_directory` char(1) NOT NULL,
  `file_regex_pattern` varchar(100) NOT NULL,
  `site_name` varchar(100) NOT NULL,
  `site_name_regex_pattern` varchar(100) NOT NULL,
  `date_from` datetime NULL default &apos;1900-01-01 00:00:00&apos;,
  `date_to` datetime NULL default  &apos;2199-12-31 23:59:59&apos;,
  `modified_on` timestamp NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  UNIQUE KEY `common_path_dates` (`common_path`,`date_from`,`date_to`)
) ENGINE=MyISAM  DEFAULT CHARSET=ascii;

INSERT IGNORE INTO `directories` (`id`, `common_path`, `im-log03_path`, `im-log02_path`, `scan_directory`, `file_regex_pattern`, `site_name`, `site_name_regex_pattern`) VALUES
(1, &apos;&lt;invalid&gt;&apos;, NULL, NULL, &apos;N&apos;, &apos;^$&apos;, &apos;&lt;unknown&gt;&apos;, &apos;^$&apos;);

CREATE TABLE IF NOT EXISTS sites (
  id smallint(5) unsigned NOT NULL auto_increment PRIMARY KEY,
  site_name varchar(100) NOT NULL UNIQUE KEY,
