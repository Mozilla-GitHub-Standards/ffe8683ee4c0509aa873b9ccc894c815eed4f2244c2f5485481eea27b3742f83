    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.regex.*;

int outputRowSize = 0;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
		outputRowSize = data.outputRowMeta.size();
    }

    String site_name = get(Fields.In, "site_name").getString(r);
	if (site_name.equals("<regex>")) {
		incrementLinesInput();
		String filename = get(Fields.In, "filename").getString(r);
	    Matcher m = Pattern.compile(get(Fields.In, "site_name_regex_pattern").getString(r)).matcher(filename);
		if (m.matches()) {
			incrementLinesUpdated();
			site_name = m.group(1);
		} else {
			incrementLinesRejected();
			site_name = "<unknown>";
		}
		get(Fields.Out, "site_name").setValue(r, site_name);
	}

    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
        </definition>
