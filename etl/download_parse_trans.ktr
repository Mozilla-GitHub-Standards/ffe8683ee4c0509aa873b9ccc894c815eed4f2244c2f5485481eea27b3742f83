  <step>
    <name>Regex DMO</name>
    <type>RegexEval</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <script><![CDATA[^(?>([^\s]{7,15}))              	# ip
\s(-)(\s)                      		# ident auth
(?>-|(?>,\s)?([^-](?:[^\[\s]++(?:(?!\s\[)[\[\s])?)++))\s      # Cookies field (unquoted so harder to parse)
\[(\d{2}/\w{3}/\d{4}                 	# (request date dd/MMM/yyyy
:\d{2}:\d{2}:\d{2}\s[-+ ]\d{4})\]    	#  request time :HH:mm:ss -0800) --> one field
\s"([A-Z]++)\s				# HTTP request type
(?>/\?([^%\s]+))                 	# URL TODO: include '/?' ?
\s([^"\s]++)"\s				# end of the request method/uri/version
(?>(\d++)|-)\s				# HTTP status code
(\d+)\s                       # bytes returned
(?>"([^"]++))"\s                 # referrer field sometimes a redirect from https://aus2.mozilla.org
"                              # User agent parsing, always quoted.
"?                             # If people spoof the user agent, they sometimes put in extra quotes.
(.*)                           # UA string
"?"                            # Closing quotes for UA string]]></script>    <matcher>line</matcher>
    <resultfieldname>parse_result</resultfieldname>
