         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <script><![CDATA[^(?>
[^\s]*                                 # dummy field: hostname
\s([\d.]{7,15})                        # client_ip
\s([^\s]+)                             # auth
\s([^\s]+)                             # ident
\s\[(\d{2}/\w{3}/\d{4}                   # request date dd/MMM/yyyy
:\d{2}:\d{2}:\d{2}\s[-+ ]\d{4})\]      # request time :HH:mm:ss -0800
)

\s"(?:(?>
([A-Z]++)                              # HTTP request type
\s([^\s]++)                            # URL
\s([^"\s]++)                           # end of the request method/uri/version
)
|\s\s)"

(?>
\s(\d++|-)                             # HTTP status code
\s(\d++)                               # bytes returned
)

(?>\s"([^"]++)")                       # referrer field

(?>
\s""?                                  # User agent parsing, always quoted, sometimes double
([^"]*)                                # UA string
""?                                    # Closing quotes for UA string
)

(?>\s"(.*)"$)                          # cookies field
]]></script>    <matcher>line</matcher>
